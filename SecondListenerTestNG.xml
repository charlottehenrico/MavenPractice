<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ListenerSuite">
	<listeners>
		<listener class-name="extraListener.newListenerClass"></listener>
	</listeners>
	<test thread-count="5" name="ListenerClass">
		<groups>
			<run>
				<exclude name="Skip" />
			</run>
		</groups>
		<classes>
			<class name="extraListener.listenerClass" />
		</classes>
	</test> <!-- ListenerTest -->
</suite> <!-- ListenerSuite -->
